SELECT market_id, event, sportsbook_id, participant_id, datetime, american_odds, decimal_odds, event_id, market_id, participant_id, participant_score, profit, result FROM OPENINGLINES LIMIT 100;
SELECT * FROM LINEHISTORY LIMIT 100;
